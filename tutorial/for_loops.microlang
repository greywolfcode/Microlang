#for loop. Stops one before the stopping value
for x = 0 , 10 do
{
    display x
}

#can provide a step value to iterate by
for x = 0 , 10 , 2 do
{
    display x
}

#use break to exit loop early
for x = 0 , 20 do
{
    display x
    if ( x == 10 ) then
    {
        break
    }
}

#can loop through arrays with for loop
make array a = { 0 , 9 , 8 }
for num in a do 
{
    display num
}

#can use an array not in a variables
for item in { 'hi' , 8 , 7 } do
{
    display item
}