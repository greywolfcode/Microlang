#create an array
make array greetings = { 'hi' , 'hello' , 'hola' , 'bonjour' , 'aloha' }

#get value from array. Starts at index zero
make str englishGreeting = greetings ( 0 )
#this is an appropriate time to use the var type
make var spanishGreeting = greetings ( 2 )

#2d array example
make array thisIsATwoDArray = { { 1 , 2 , 3 } , { 4 , 5 , 6 } }
make flt num = thisIsATwoDArray ( 0 ) ( 1 )