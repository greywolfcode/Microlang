#basic function:
#func NAME [ ARGS ]
#arguments must have type specified, and specify type to return
func new_func [ flt arg1 , str arg2 ] -> void
{
    #what the function does
    display arg1
    display arg2
}
#run function
new_func [ 5 , 'Hello!' ]
#returning variables: can only return one thing
func return_var [ ] -> str
{
    return 'Hi'
}
make str a = return_var [ ]
display a