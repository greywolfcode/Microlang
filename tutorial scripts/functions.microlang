#basic function:
#func NAME [ ARGS ]
#arguments must have type specified, and specify type to return
func newFunc [ flt arg1 , str arg2 ] -> void
{
    #what the function does
    display arg1
    display arg2
}
#run function
newFunc [ 5 , 'Hello!' ]

#returning data/variables: can only return one object
func return_var [ ] -> str
{
    return 'Hi'
}
#run function
make str a = return_var [ ]
display a