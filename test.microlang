class classOne [ ]
{
    func __init__ [ flt one , flt two ] -> void
    {
        make flt self . one = one
        make flt self . two = two
    }
    func add [ ] -> flt
    {
        make flt c = self . one + self . two
        return c
    }
}

make instance d = classOne [ 9 , 7 ]
make flt e = d . add []